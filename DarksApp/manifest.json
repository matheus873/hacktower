{
  "manifest_version": 2,
  "name": "DarksApp",
  "description": "Dark is best",
  "version": "1.0.0",
  "icons": {
    "128": "images/icon1.png"
  },
  "browser_action": {
    "default_icon": "images/icon1.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/",
    "https://*/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://web.whatsapp.com/"
      ]
    }
  ]
}